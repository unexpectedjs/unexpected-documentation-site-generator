  "dependencies": {
    "async": "3.2.4",
    "autoprefixer": "9.8.6",
    "debug": "4.1.0",
    "ejs": "2.5.6",
    "evaldown": "^1.4.0",
    "gh-pages": "2.0.1",
    "glob": "9.3.5",
    "lodash": "4.17.19",
    "magicpen-prism": "4.0.0",
    "metalsmith": "1.7.0",
    "metalsmith-autoprefixer": "1.1.1",
    "metalsmith-collections": "0.9.0",
    "metalsmith-less": "2.0.0",
    "metalsmith-markdown": "0.2.2",
    "metalsmith-permalinks": "0.5.0",
    "metalsmith-relative": "1.0.3",
    "metalsmith-static": "0.0.5",
    "metalsmith-templates": "0.7.0",
    "minimatch": "3.0.3",
    "minimist": "1.2.5",
    "passerror": "1.1.1",
    "rimraf": "3.0.2",
    "stat-mode": "0.2.2"
  },
  "directories": {
    "bin": "bin"
  },
  "devDependencies": {
    "eslint": "5.8.0",
    "eslint-config-pretty-standard": "2.0.0",
    "eslint-plugin-prettier": "3.0.0",
    "offline-github-changelog": "^1.6.1",
