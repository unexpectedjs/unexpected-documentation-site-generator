    "passerror": "1.1.1",
    "rimraf": "3.0.2",
    "stat-mode": "0.2.2"
  },
  "directories": {
    "bin": "bin"
  },
  "devDependencies": {
    "eslint": "5.8.0",
    "eslint-config-pretty-standard": "2.0.0",
    "eslint-plugin-prettier": "3.0.0",
    "offline-github-changelog": "^3.0.1",
    "prettier": "1.14.3"
